{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "trailing-comma": [true, { "typeLiterals": "ignore" }]
    },
    "rulesDirectory": []
}